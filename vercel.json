{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/huong-dan-lam-tieu-ban-thuc-vat-hoa-kho",
      "destination": "/tieu-ban-thuc-vat-hoa-kho",
      "permanent": true
    },
    {
      "source": "/huong-dan-dang-ky-google-drive-google-one-gia-re-tai-tho-nhi-ky",
      "destination": "/google-one-tho-nhi-ky",
      "permanent": false
    }
  ]
}
