---
import Layout from '~/layouts/page.astro';

import Address from '~/components/content/analytics/address.astro';

import Action from '~/components/content/main/action.astro';

const metadata = {
	title: 'Thông tin địa chỉ IP của tôi là gì?',
	description: 'Công cụ tra cứu thông tin địa chỉ IP là giải pháp hữu hiệu giúp người dùng nhanh chóng biết được địa chỉ IP của mình để có thể tiến hành ẩn thông tin kịp thời.',
};
---

<Layout metadata={metadata}>

	<Address/>

	<Action/>

</Layout>