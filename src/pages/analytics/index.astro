---
import Layout from '~/layouts/page.astro';

import Hero from '~/components/content/hero.astro';

import Compare from '~/components/content/analytics/compare.astro';
import Count from '~/components/content/analytics/count.astro';
import Emoji from '~/components/content/analytics/emoji.astro';
import Integrate from '~/components/content/analytics/integrate.astro';
import Lower from '~/components/content/analytics/lower.astro';
import Normalize from '~/components/content/analytics/normalize.astro';
import Unicode from '~/components/content/analytics/unicode.astro';

import Action from '~/components/content/analytics/action.astro';

const metadata = {
	title: 'Công cụ nâng cao',
};
---

<Layout metadata={metadata}>

	<Hero/>

	<Integrate/>

	<Compare/>

	<Normalize/>

	<Count/>

	<Lower/>

	<Emoji/>

	<Unicode/>

	<Action/>

</Layout>