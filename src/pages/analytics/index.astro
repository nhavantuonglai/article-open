---
import Layout from '~/layouts/page.astro';

import Hero from '~/components/content/hero.astro';

import Compare from '~/components/content/compare.astro';
import Count from '~/components/content/count.astro';
import Emoji from '~/components/content/emoji.astro';
import Integrate from '~/components/content/integrate.astro';
import Lower from '~/components/content/lower.astro';
import Normalize from '~/components/content/normalize.astro';
import Unicode from '~/components/content/unicode.astro';

import Action from '~/components/content/action.astro';

import Soundcloud from '~/components/javascript/soundcloud.astro';

const metadata = {
	title: 'Công cụ nâng cao',
};
---

<Layout metadata={metadata}>

	<Hero/>

	<Integrate/>

	<Compare/>

	<Normalize/>

	<Count/>

	<Lower/>

	<Emoji/>

	<Unicode/>

	<Action/>

	<Soundcloud/>

</Layout>